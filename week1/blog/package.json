{
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo": "7.3.0",
    "@popperjs/core": "^2.11.8",
    "@rails/actiontext": "^7.1.0",
    "bootstrap": "^5.3.8",
    "sass": "^1.94.2",
    "trix": "^2.1.15"
  },
  "scripts": {
